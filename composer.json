{
    "name": "pickles2/px2-publish-ex",
    "description": "複数のデバイス向けのバージョンをパブリッシュできる、 Pickles 2 用のパブリッシュプラグイン。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "scripts":{
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ] ,
        "documentation": [
            "php ./vendor/phpdocumentor/phpdocumentor/bin/phpdoc --title \"pickles2/px2-publish-ex v2.0.3 API Document\" -d \"./php/\" -t \"./docs/phpdoc/\""
        ]
    } ,
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
    ],
    "require": {
    },
    "require-dev": {
        "pickles2/px-fw-2.x": "dev-develop",
        "pickles2/px2-multitheme": "dev-develop",
        "phpdocumentor/phpdocumentor": "^2.9",
        "phpunit/phpunit": "^4.8",
        "jms/serializer": "<1.8"
    },
    "autoload": {
        "files": [
            "php/device_target_path.php",
            "php/path_resolver.php",
            "php/path_rewriter.php",
            "php/publish.php",
            "php/tmp_publish_dir.php",
            "php/simple_html_dom.php"
        ]
    }
}
