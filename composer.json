{
    "name": "pickles2/px2-publish-ex",
    "description": "複数のデバイス向けのバージョンをパブリッシュできる、 Pickles 2 用のパブリッシュプラグイン。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "scripts":{
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ] ,
        "documentation": [
            "php ./vendor/phpdocumentor/phpdocumentor/bin/phpdoc --title \"pickles2/px2-publish-ex v2.1.0 API Document\" -d \"./php/\" -t \"./docs/phpdoc/\""
        ]
    } ,
    "config": {
        "process-timeout": 0
    },
    "minimum-stability": "dev",
    "repositories": [
    ],
    "require": {
        "php": ">=7.3.0"
    },
    "require-dev": {
        "pickles2/px-fw-2.x": "~2.1||~2.0",
        "pickles2/px2-multitheme": "~2.0",
        "phpdocumentor/phpdocumentor": "~3.0.0",
        "phpunit/phpunit": "~9.5",
        "jms/serializer": "<1.8"
    },
    "autoload": {
        "psr-4": {
            "tomk79\\pickles2\\publishEx\\": [
                "php"
            ]
        },
        "files": [
            "php/simple_html_dom.php"
        ]
    }
}
